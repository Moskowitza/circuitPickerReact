{
  "name": "circuit-picker",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "4.1.0",
    "react-scripts": "^2.1.1"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "firebase": "^5.7.0",
    "firebaseui": "^3.5.1",
    "pretty-ms": "^4.0.0",
    "prop-types": "^15.6.2",
    "re-base": "^4.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-transition-group": "^2.5.1",
    "serve": "^10.1.1",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve --single ./build",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/style.styl -o ./src/css/style.css",
    "styles:watch": "stylus -w ./src/css/style.styl -o ./src/css/style.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
